<div class="gp-discount gp-discont--company">
	<div class="arrow-down arrow-down--white">
		<div class="arrow-down__wrapper">
			<span>
				{% sw_include '@ThStarterPlugin/storefront/block/icon-down-white.html.twig' %}
			</span>
			<span class="arrow-down__label">Shop</span>
		</div>
	</div>
