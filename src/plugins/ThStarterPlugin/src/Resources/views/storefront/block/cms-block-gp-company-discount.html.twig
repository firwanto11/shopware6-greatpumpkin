			<div class="col-md-4">
				<div class="gp-discount__card">
					{% set element = block.slots.getSlot('first-title') %}
					{% if element %}
						<div data-cms-element-id="{{ element.id }}" class="gp-discount__card-title">
							{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
						</div>
